{
  "extends": [
    "tslint:latest",
    "tslint-config-prettier",
    "tslint-config-airbnb"
  ],
  "rules": {
    "quotemark": [true, "single", "avoid-escape", "avoid-template"],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ]
  }
}
